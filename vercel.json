{
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/photovault/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_CONFIG": "production",
    "USE_EXTERNAL_STORAGE": "true"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  }
}